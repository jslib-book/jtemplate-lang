{
    "name": "jtemplate-lang",
    "displayName": "jtemplate lang",
    "description": "Syntax highlighting for jtemplate",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jtemplate",
            "aliases": ["jtemplate", "jtemplate"],
            "extensions": [".tmpl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jtemplate",
            "scopeName": "source.tmpl",
            "path": "./syntaxes/jtemplate.tmLanguage.json"
        }]
    }
}